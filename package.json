{
  "name": "camhome-surveillance",
  "version": "1.0.0",
  "description": "Dashboard de vigilancia Orange Pi - CamHome",
  "scripts": {
    "dev": "parcel index.html",
    "build": "rm -rf dist .parcel-cache && parcel build index.html --dist-dir dist --public-url /",
    "start": "node server.js",
    "start:prod": "pm2 start ecosystem.config.js",
    "stop:prod": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs camhome-backend"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "express": "^4.19.2",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.3",
    "buffer": "^6.0.3",
    "events": "^3.3.0"
  }
}
